---
layout: null
---
{
  {% for item_hash in site.data.audio %}
    {% assign object = item_hash[1] %}
    {% assign id = item_hash[0] %}
    {% assign avnumber = object.instances[0].container.indicator_1 %}
    "audio/{{id}}": {
      "href": "audio/{{id}}",
      "title": "{{object.title | escape | strip_newlines | remove: '\'}}",
      "avnumber": "{{ avnumber }}",
      "url": "{{site.url}}/audio/{{id}}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
}
