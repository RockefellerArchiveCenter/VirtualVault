---
layout: null
---
{
  {% for item_hash in site.data.moving-image %}
    {% assign object = item_hash[1] %}
    {% assign id = item_hash[0] %}
    {% assign avnumber = object.instances[0].container.indicator_1 %}
    "moving-image/{{id}}": {
      "href": "moving-image/{{id}}",
      "title": "{{object.title | escape | strip_newlines | remove: '\'}}",
      "avnumber": "{{ avnumber }}",
      "url": "{{site.url}}/moving-image/{{id}}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
}
